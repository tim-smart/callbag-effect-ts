{
  "name": "callbag-effect-ts",
  "version": "0.9.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "yarn clean && ttsc && yarn build:esm",
    "build:esm": "ttsc -p ./tsconfig.mjs.json",
    "clean": "git clean -fxd -e node_modules"
  },
  "files": [
    "**/*.js",
    "**/*.js.map",
    "**/*.ts",
    "!test"
  ],
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./esm/index.js"
    },
    "./Sink": {
      "require": "./Sink/index.js",
      "import": "./esm/Sink/index.js"
    }
  },
  "devDependencies": {
    "@effect-ts/node": "^0.38.3",
    "@effect-ts/tracing-plugin": "^0.19.1",
    "@types/node": "^17.0.21",
    "lerna": "^4.0.0",
    "ts-node": "^10.7.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@effect-ts/core": "^0.58.0",
    "strict-callbag": "^0.9.0",
    "strict-callbag-basics": "^0.3.0"
  }
}
