{
  "project": {
    "packageName": "callbag-effect-ts",
    "rootDir": "src",
    "tsconfig": "tsconfig.json",
    "exclude": ["**/internal/**", "**/_internal/**"],
    "staticPrefixes": ["from"],
    "fluentNamespaces": ["callbag-effect-ts", "effect/io"]
  },
  "namespaces": [
    {
      "name": "callbag-effect-ts",
      "type": {
        "include": true,
        "includeCompanion": true,
        "companionSuffix": ".Ops"
      },
      "static": {
        "include": true,
        "staticSuffix": ".Ops"
      }
    },
    {
      "name": "effect/core/io"
    }
  ],
  "additionalExtensions": []
}
